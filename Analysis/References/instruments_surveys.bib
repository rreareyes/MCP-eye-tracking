@article{jekelHowIdentifyStrategy2018,
	title = {How to {Identify} {Strategy} {Use} and {Adaptive} {Strategy} {Selection}: {The} {Crucial} {Role} of {Chance} {Correction} in {Weighted} {Compensatory} {Strategies}: {How} to {Identify} {Strategy} {Use} and {Adaptive} {Strategy} {Selection}},
