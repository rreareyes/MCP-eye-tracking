	language = {en},
	urldate = {2020-07-07},
	journal = {Journal of Immunology Research},
	author = {Muñoz, Fabiola Carolina and Cervantes, Maritza Montserrat and Cervantes-García, Daniel and Jiménez, Mariela and Ventura-Juárez, Javier and Salinas, Eva},
